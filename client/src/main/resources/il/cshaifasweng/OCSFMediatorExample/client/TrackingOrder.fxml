<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="parent" prefHeight="480.0" prefWidth="800.0" styleClass="primary-color" stylesheets="@../../../../Styles/LightMode.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.TrackingOrder">
   <children>
      <Button fx:id="SendBtn" layoutX="375.0" layoutY="378.0" mnemonicParsing="false" onAction="#sendTracking" prefHeight="26.0" prefWidth="58.0" text="send">
         <styleClass>
            <String fx:value="secondary-color" />
            <String fx:value="round-border" />
         </styleClass></Button>
      <TextField fx:id="IdTF" layoutX="297.0" layoutY="209.0" prefHeight="25.0" prefWidth="206.0">
         <styleClass>
            <String fx:value="secondary-color" />
            <String fx:value="round-border" />
         </styleClass></TextField>
      <TextField fx:id="CarNumberTF" layoutX="297.0" layoutY="274.0" prefHeight="25.0" prefWidth="206.0">
         <styleClass>
            <String fx:value="secondary-color" />
            <String fx:value="round-border" />
         </styleClass></TextField>
      <Label layoutX="253.0" layoutY="208.0" text="Id*:">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label layoutX="179.0" layoutY="274.0" text="Car Number:">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label layoutX="290.0" layoutY="114.0" text="Tracking Order ">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <ToolBar accessibleRole="MENU" focusTraversable="false" layoutY="-4.0" prefHeight="62.0" prefWidth="806.0" styleClass="secondary-color">
         <padding>
            <Insets left="10.0" />
         </padding>
      </ToolBar>
      <Button fx:id="BackToMain" layoutX="735.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToMainMenu" prefHeight="30.0" prefWidth="59.0" text="Back">
         <styleClass>
            <String fx:value="primary-color" />
            <String fx:value="round-border" />
            <String fx:value="text-field" />
         </styleClass>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnMode" layoutX="14.0" layoutY="12.0" minWidth="32.0" mnemonicParsing="false" onAction="#changeMode" prefHeight="26.0" prefWidth="32.0">
         <styleClass>
            <String fx:value="primary-color" />
            <String fx:value="round-border" />
         </styleClass>
         <graphic>
            <ImageView fx:id="imMode" fitHeight="77.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../images/LightMode2.png" />
               </image>
               <styleClass>
                  <String fx:value="round-border" />
                  <String fx:value="primary-color" />
               </styleClass>
            </ImageView>
         </graphic>
      </Button>
   </children>
